{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/navigation/*": [
        "src/navigation/*"
      ],
      "@/store/*": [
        "src/store/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    },
  },
}
